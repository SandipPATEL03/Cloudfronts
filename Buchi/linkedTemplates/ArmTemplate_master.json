{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Buchi"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"BuchiAzureSqlDataBase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BuchiAzureSqlDataBase'"
		},
		"DynamicsCRMCFT130_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DynamicsCRMCFT130'"
		},
		"OnPremSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremSQLServer'"
		},
		"DynamicsCRMCFT130_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "crmadmin@cft130.onmicrosoft.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"BuchiAzureSqlDataBase_connectionString": {
						"value": "[parameters('BuchiAzureSqlDataBase_connectionString')]"
					},
					"DynamicsCRMCFT130_password": {
						"value": "[parameters('DynamicsCRMCFT130_password')]"
					},
					"OnPremSQLServer_connectionString": {
						"value": "[parameters('OnPremSQLServer_connectionString')]"
					},
					"DynamicsCRMCFT130_properties_typeProperties_username": {
						"value": "[parameters('DynamicsCRMCFT130_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}